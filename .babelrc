{
    "presets": [
        [
            "@babel/env",
            {
                "modules": false,
                "loose": true,
                "targets": {
                    "browsers": [">1%", "Chrome 41"]
                },
                "useBuiltIns": "entry"
            }
        ],
        "@babel/typescript"
    ],
    "plugins": [
        "@babel/syntax-dynamic-import",
        "@babel/transform-react-jsx",
        "transform-react-display-name",
        ["transform-class-properties", { "spec": true }],
        "react-hot-loader/babel",
        "lodash",
        "recharts"
    ]
}
