{
    "presets": [
        [
            "@babel/env",
            {
                "modules": false,
                "loose": true,
                "targets": {
                    "browsers": [">1%", "Chrome 41"]
                },
                "useBuiltIns": "entry"
            }
        ]
    ],
    "plugins": [
        "@babel/syntax-dynamic-import",
        "@babel/transform-react-jsx"
    ]
}
