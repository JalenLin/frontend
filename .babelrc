{
    "presets": [
        [
            "env",
            {
                "modules": false,
                "loose": true,
                "targets": {
                    "browsers": [">1%", "Chrome 41"]
                },
                "useBuiltIns": true
            }
        ]
    ],
    "plugins": [
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        "transform-react-jsx"
    ]
}
