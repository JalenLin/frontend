{
  "name": "gitgudscrub-frontend",
  "scripts": {
    "start": "webpack-dev-server --progress --hot --inline --config config/dev.js",
    "watch": "webpack --progress --watch --config config/dev.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config config/dev.js",
    "build": "cross-env BROWSERLIST_STATS=./browserlist-stats.json NODE_ENV=production webpack --config config/prod.js",
    "stats": "webpack --config config/dev.js --profile --json > stats.json",
    "test": "exit 0"
  },
  "dependencies": {
    "chartist": "^0.11.0",
    "chartist-plugin-tooltips": "^0.0.17",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "lozad": "^1.0.2",
    "mithril": "^1.1.1",
    "redux": "^3.7.2",
    "redux-first-router": "^1.9.19",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.9.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.5.3",
    "postcss-discard-duplicates": "^2.0.2",
    "postcss-loader": "^1.1.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "svg-sprite-loader": "2.1.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
