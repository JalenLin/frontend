{
  "name": "gitgudscrub-frontend",
  "scripts": {
    "start": "webpack-dev-server --progress --hot --inline --config config/dev.js",
    "watch": "webpack --progress --watch --config config/dev.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config config/dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/prod.js",
    "stats": "webpack --config config/dev.js --profile --json > stats.json",
    "test": "jest",
    "test:cli": "jest",
    "test:browser": "testcafe remote test/browser/**/*.js -S -s screenshots --touch-mode"
  },
  "jest": {
    "moduleNameMapper": {
      "^lib(.*)$": "<rootDir>/src/app/lib$1",
      "^components(.*)$": "<rootDir>/src/app/components$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/testUtil/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/testUtil/styleMock.js"
    },
    "testURL": "https://r6db.com",
    "testRegex": "test/cli/.*\\.(spec|test)\\.js$"
  },
  "dependencies": {
    "@larsbrix/logger": "^3.1.0",
    "baobab": "^2.3.3",
    "isomorphic-fetch": "^2.2.1",
    "mithril": "github:lhorie/mithril.js#rewrite",
    "moment": "^2.15.1",
    "page": "^1.7.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.5.2",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.9.1",
    "eslint": "^3.14.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "happypack": "^2.2.1",
    "html-webpack-inline-source-plugin": "^0.0.6",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^18.0.0",
    "mithril-tidy": "^0.1.0",
    "node-sass": "^3.12.5",
    "postcss-discard-duplicates": "^2.0.2",
    "postcss-loader": "^1.1.1",
    "resource-hints-webpack-plugin": "^0.0.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-ext-html-webpack-plugin": "^3.0.8",
    "style-loader": "^0.13.1",
    "svg-sprite-loader": "^0.2.0",
    "testcafe": "^0.12.1",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.12",
    "webpack-uglify-parallel": "^0.1.2"
  }
}
